.scrollfield
    width: 100%;
    height: 200px;

.scrollbar {
height: 300px;
overflow-y: scroll;
margin-bottom: 25px;
}

.scrollbar-primary::-webkit-scrollbar {
width: 12px;
background-color: #971E1E; }

.scrollbar-primary::-webkit-scrollbar-thumb {
border-radius: 10px;
-webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.1);
background-color: #0d0d0d; }

p {
    text-align: left;
}



