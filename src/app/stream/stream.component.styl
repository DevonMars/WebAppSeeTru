primary-border = 2px solid 
primary-radius = 4px
primary-color = rgba(217, 30, 24, 0.8)

.rightbox
    height 85vh
    border-radius primary-radius

.stream-list-item
    height 64px;
    width 100%
    border primary-border
    border-radius: primary-radius
    

.stream-list-item img 
    width: 100%
    height: 100%
    border-radius: primary-radius
   

.stream-list-item .col-md-4
    padding 0 !important
    height: 60px
   

